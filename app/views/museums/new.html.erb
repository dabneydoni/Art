<h1>New Museum</h1>

<%= form_for :museum, url: museums_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :location %><br>
    <%= f.text_field :location %>
  </p>
 
  <p>
    <%= f.label :hours %><br>
    <%= f.text_area :hours %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>